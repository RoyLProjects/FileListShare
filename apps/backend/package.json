{
  "name": "backend",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "api:spec": "tsx src/openapi.ts",
    "start": "node dist/index.js",
    "generate:custom": "prisma generate --schema=prisma/app/schema.prisma",
    "generate:auth": "prisma generate --schema=prisma/auth/schema.prisma",
    "generate:all": "pnpm generate:custom && pnpm generate:auth",
    "migrate:custom": "prisma migrate dev --schema=prisma/app/schema.prisma",
    "migrate:auth": "prisma migrate dev --schema=prisma/auth/schema.prisma",
    "migrate:all": "pnpm migrate:custom && pnpm migrate:auth",
    "migrate:custom:prod": "node ./scripts/run-migrations.mjs custom",
    "migrate:auth:prod": "node ./scripts/run-migrations.mjs auth",
    "migrate:all:prod": "pnpm migrate:custom:prod && pnpm migrate:auth:prod",
    "format": "pnpm prettier --write .",
    "format:check": "pnpm prettier --check ."
  },
  "dependencies": {
    "@prisma/client": "^5.19.1",
    "argon2": "^0.44.0",
    "better-auth": "^1.3.34",
    "cookies": "^0.9.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-zod-api": "^26.0.0",
    "helmet": "^8.1.0",
    "install": "^0.13.0",
    "pino": "^9.0.0",
    "pino-http": "^11.0.0",
    "pino-loki": "^2.6.0",
    "pino-pretty": "^13.1.2",
    "prom-client": "^15.1.3",
    "redis": "^5.9.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@jridgewell/trace-mapping": "^0.3.31",
    "@types/cookies": "^0.9.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^5.0.5",
    "@types/node": "^20.19.25",
    "@vitest/ui": "^4.0.9",
    "compression": "1.8",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "express-fileupload": "1.5",
    "prisma": "^7.1.0",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.17.0",
    "typescript": "^5.6.3",
    "vitest": "^4.0.9"
  }
}
